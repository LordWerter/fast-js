{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es5",
            "dom",
            "es2015.iterable",
            "es2015.promise"
        ],

        "noImplicitAny": false,
        "noImplicitThis": false,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "baseUrl": ".",
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
